# Settings
description: Sample mapping
s_layer_left: japanese_eisuu
s_layer_right: japanese_kana

# Layer 1
left_control + h: delete_or_backspace
left_control + m: return_or_enter
right_command: escape

# Layer 2
## Row 1
LL,LR + q: 1
LL,LR + w: 2
LL,LR + e: 3
LL,LR + r: 4
LL,LR + t: 5

LL,LR + y: 6
LL,LR + u: 7
LL,LR + i: 8
LL,LR + o: 9
LL,LR + p: 0
LL,LR + close_bracket: grave_accent_and_tilde

## Row 2
LL,LR + a: left_shift + 1 # !
LL,LR + s: left_shift + 2 # @
LL,LR + d: left_shift + 3 # #
LL,LR + f: left_shift + 4 # $
LL,LR + g: left_shift + 5 # %

LL,LR + h: left_shift + 6 # ^ 
LL,LR + j: left_shift + 7 # &
LL,LR + k: left_shift + 8 # *
LL,LR + l: left_shift + 9 # (
LL,LR + semicolon: left_shift + 0 # )

## Row 3
LL,LR + z: backslash
LL,LR + x: open_bracket
LL,LR + c: close_bracket
LL,LR + v: hyphen
LL,LR + b: left_shift + equal_sign # !

LL,LR + n: left_shift + hyphen # _
LL,LR + m: equal_sign # =
LL,LR + comma: left_shift + open_bracket # {
LL,LR + period: left_shift + close_bracket # }
LL,LR + slash: left_shift + backslash # |

# Layer 3
LL + LR + h: grave_accent_and_tilde 
LL + LR + j: left_shift + grave_accent_and_tilde 
LL + LR + k: backslash 
LL + LR + l: tab 

